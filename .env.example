# Engrave Protocol - Environment Variables Template
# Copy this file to .env and fill in your values

# Treasury wallet address on Solana (receives x402 payments)
TREASURY_WALLET_ADDRESS=your_solana_wallet_address_here

# Base API URL (e.g., http://localhost:3000 or https://yourdomain.com)
BASE_API_URL=http://localhost:3000

# Server port
PORT=3000

# Optional: Node environment
NODE_ENV=development
